{
  "version": "1.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eikaramba/multilingual-number-parser.git"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "vitest",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "i18n-number-parser",
  "author": "Eike Thies",
  "module": "dist/multilingual-number-parser.esm.js",
  "size-limit": [
    {
      "path": "dist/multilingual-number-parser.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/multilingual-number-parser.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "chai": "^5.2.0",
    "husky": "^9.1.7",
    "size-limit": "^11.2.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.8.1",
    "vitest": "^3.2.3",
    "typescript": "^5.8.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
